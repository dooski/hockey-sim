{
  "name": "hockey-sim",
  "version": "1.0.0",
  "engines": {
"node": "14.5.3"
  },
  "description": "blaseball-esque hockey simulator",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon -q\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dooski/hockey-sim.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dooski/hockey-sim/issues"
  },
  "homepage": "https://github.com/dooski/hockey-sim#readme",
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "axios": "^0.21.4",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.6.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "fs": "0.0.1-security",
    "if-env": "^1.0.4",
    "moment": "^2.29.4",
    "mongoose": "^5.13.15",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "react-emoji-render": "^1.2.4",
    "rng-js": "^1.0.1",
    "styled-components": "^5.3.6"
  }
}
